{
  "name": "react-fundamentals-curriculum",
  "version": "1.0.0",
  "description": "Curriculum for React Training's React Fundamentals course.",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "transform-class-properties"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --open",
